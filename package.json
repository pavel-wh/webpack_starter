{
  "name": "webpack_starter",
  "version": "1.0.0",
  "description": "webpack with babel starter",
  "main": "index.js",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --config webpack.config.babel.js --open",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.config.babel.js",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "author": "pavel-wh <pavel.wh@yandex.ru>",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "@": "./src",
            "@nm": "./node_modules"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-unused-vars": "warn"
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.3",
    "favicons-webpack-plugin": "^6.0.0",
    "file-loader": "^6.0.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-webp": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^9.0.0",
    "nodemailer": "^6.4.10",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "purgecss-webpack-plugin": "^2.3.0",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sass-loader": "^8.0.2",
    "sitemap-webpack-plugin": "^0.8.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webp-loader": "^0.6.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^5.1.3",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "vue": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavel-wh/webpack_starter.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack",
    "babel"
  ],
  "bugs": {
    "url": "https://github.com/pavel-wh/webpack_starter/issues"
  },
  "homepage": "https://github.com/pavel-wh/webpack_starter#readme"
}
